{
    "extends": "../../tsconfig.json",
    "include": ["src"],
    "compilerOptions": {
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "noEmit": true,
        "jsx": "preserve",
        "jsxImportSource": "vue",
        "allowImportingTsExtensions": true,
        "resolvePackageJsonImports": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"]
        }
    }
}
